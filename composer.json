{
    "name": "owenvoke/mnemonics-php",
    "type": "library",
    "description": "A mnemonics generator for PHP 7.2+",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Mnemonics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Mnemonics\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
